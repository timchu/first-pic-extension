{
  "manifest_version": 3,
  "name": "ContentToPopup Test",
  "version": "1.0",
  "permissions": ["tabs", "storage"],
  "web_accessible_resources": [{
    "resources": ["normal_popup.html"],
    "matches": [
              "https://*/*",
              "http://*/*"
    ]
  }],
  "content_scripts": [{
      "js": ["content.js"],
      "matches": [
          "https://*/*",
          "http://*/*"
      ]
  }]
}
